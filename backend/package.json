{
  "name": "devbasenestprisma",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docker:up": "docker-compose -f '../db/docker-compose.yml' --env-file ../.env up -d --build",
    "docker:down": "docker-compose -f '../db/docker-compose.yml' --env-file ../.env down",
    "start:db": "npm run prisma:generate && npm run prisma-migrate",
    "start:dev": "dotenv -e ../.env -- nest start --watch",
    "start:dev-i18copy": "npm run copy-assets && npm run start:dev",
    "nx": "pnpm nx",
    "nxupdate": "nx migrate latest",
    "nxmigrate": "nx migrate --run-migrations",
    "pnpm-auto-update": "pnpm update",
    "prisma:studio": "dotenv -e ../.env -- npx prisma studio",
    "seedold": "dotenv -e ../.env ts-node prisma/seed-old.ts",
    "seed": "dotenv -e ../.env ts-node prisma/seed.ts",
    "seedFromFiles": "npx prisma db seed",
    "pnpm-instal": "pnpm install --no-frozen-lockfile",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "copy-assets": "cp -rv src/i18n dist/src/",
    "copy-prisma": "cp -rv ./node_modules/.prisma ../frontend/src/app/",
    "start": "nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "prisma-migrate": "dotenv -e ../.env -- npx prisma migrate dev"
  },
  "dependencies": {
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/throttler": "^6.3.0",
    "@prisma/client": "^5.22.0",
    "acorn": "8.14.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^7.4.4",
    "express": "4.21.1",
    "fs-extra": "^11.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "nestjs-i18n": "^10.5.0",
    "nodemailer": "6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "password-hash-and-salt": "^0.1.4",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "swagger-ui-express": "^5.0.1",
    "typesync": "^0.13.4",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@faker-js/faker": "9.2.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "10.4.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^9.6.1",
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/ms": "^0.7.34",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/password-hash-and-salt": "^0.1.2",
    "@types/prettier": "^3.0.0",
    "@types/rimraf": "^4.0.5",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/validator": "13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nestjs-prisma": "^0.23.0",
    "pnpm": "^9.15.0",
    "prettier": "^3.4.2",
    "prisma": "^5.22.0",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-json-schema-generator": "^5.1.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "prisma": {
      "seed": "ts-node prisma/seed.ts"
    }
  }
}
