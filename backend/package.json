{
  "name": "devbasenestprisma",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docker:up": "docker-compose -f '../db/docker-compose.yml' --env-file ../.env up -d --build",
    "docker:down": "docker-compose -f '../db/docker-compose.yml' --env-file ../.env down",
    "start:db": "npm run prisma:generate && npm run prisma-migrate",
    "start:dev": "dotenv -e ../.env -- nest start --watch",
    "start:dev-i18copy": "npm run copy-assets && npm run start:dev",
    "prisma:studio": "dotenv -e ../.env -- npx prisma studio",
    "seed": "dotenv -e ../.env ts-node prisma/seed-old.ts",
    "seedFromFiles": "npx prisma db seed --preview-feature",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "copy-assets": "cp -rv src/i18n dist/src/",
    "copy-prisma": "cp -rv ./node_modules/.prisma ../frontend/src/app/",
    "start": "nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "prisma-migrate": "dotenv -e ../.env -- npx prisma migrate dev"
  },
  "dependencies": {
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^8.1.2",
    "@nestjs/config": "^1.1.0",
    "@nestjs/core": "^8.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.1.2",
    "@nestjs/swagger": "^5.1.4",
    "@prisma/client": "^3.4.0",
    "acorn": "8.5.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.1.0",
    "express": "4.17.1",
    "fs-extra": "^10.0.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "ms": "^2.1.3",
    "nestjs-i18n": "^8.1.6",
    "nodemailer": "6.7.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "password-hash-and-salt": "^0.1.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "sharp": "^0.29.2",
    "swagger-ui-express": "^4.1.6",
    "typesync": "^0.8.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^7.28.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.6",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/password-hash-and-salt": "^0.1.0",
    "@types/prettier": "^2.4.1",
    "@types/rimraf": "^3.0.2",
    "@types/sharp": "^0.29.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "5.5.3",
    "jest": "^27.3.1",
    "nestjs-prisma": "^0.14.3",
    "prettier": "^2.4.1",
    "prisma": "^3.4.0",
    "prisma-dbml-generator": "^0.8.1",
    "prisma-json-schema-generator": "^1.5.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "~4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
