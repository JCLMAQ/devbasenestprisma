{
  "name": "devbasenestprisma",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docker:up": "docker-compose -f '../db/docker-compose.yml' --env-file ../.env up -d --build",
    "docker:down": "docker-compose -f '../db/docker-compose.yml' --env-file ../.env down",
    "start:db": "npm run prisma:generate && npm run prisma-migrate",
    "start:dev": "dotenv -e ../.env -- nest start --watch",
    "start:dev-i18copy": "npm run copy-assets && npm run start:dev",
    "prisma:studio": "dotenv -e ../.env -- npx prisma studio",
    "seedold": "dotenv -e ../.env ts-node prisma/seed-old.ts",
    "seed": "dotenv -e ../.env ts-node prisma/seed.ts",
    "seedFromFiles": "npx prisma db seed",
    "pnpm-instal": "pnpm install --no-frozen-lockfile",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "copy-assets": "cp -rv src/i18n dist/src/",
    "copy-prisma": "cp -rv ./node_modules/.prisma ../frontend/src/app/",
    "start": "nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "prisma-migrate": "dotenv -e ../.env -- npx prisma migrate dev"
  },
  "dependencies": {
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.6",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/throttler": "^5.0.0",
    "@prisma/client": "^5.3.1",
    "acorn": "8.10.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "express": "4.18.2",
    "fs-extra": "^11.1.1",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "nestjs-i18n": "^10.3.5",
    "nodemailer": "6.9.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "password-hash-and-salt": "^0.1.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.6",
    "swagger-ui-express": "^5.0.0",
    "typesync": "^0.11.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "8.1.0",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing":"10.2.6",
    "@types/bcryptjs": "^2.4.4",
    "@types/eslint": "^8.44.3",
    "@types/eslint-plugin-prettier": "^3.1.1",
    "@types/express": "^4.17.18",
    "@types/fs-extra": "^11.0.2",
    "@types/jest": "^29.5.5",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/ms": "^0.7.32",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.8.2",
    "@types/nodemailer": "^6.4.11",
    "@types/passport": "^1.0.13",
    "@types/passport-jwt": "^3.0.10",
    "@types/passport-local": "^1.0.36",
    "@types/password-hash-and-salt": "^0.1.0",
    "@types/prettier": "^3.0.0",
    "@types/rimraf": "^4.0.5",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^2.0.14",
    "@types/uuid": "^9.0.4",
    "@types/validator": "13.11.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nestjs-prisma": "^0.22.0",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1",
    "prisma-dbml-generator": "^0.10.0",
    "prisma-json-schema-generator": "^4.0.0",

    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.1.6"

  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "prisma": {
      "seed": "ts-node prisma/seed.ts"
    }
  }
}
