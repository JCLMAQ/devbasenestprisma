{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "dotenv -e ../.env ts-node ./scripts/setenv.ts",
    "start": "npm run config -- --environment=dev && ng serve --proxy-config proxy.conf.json --port 4100",
    "build": "npm run config -- --environment=prod && ng build",
    "preinstall": "typesync || :",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,da,de,fi,nb,nl,sv}.json --clean --format json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.6",
    "@angular/cdk": "^14.2.5",
    "@angular/common": "^14.2.6",
    "@angular/compiler": "^14.2.6",
    "@angular/core": "^14.2.6",
    "@angular/forms": "^14.2.6",
    "@angular/material": "^14.2.5",
    "@angular/platform-browser": "^14.2.6",
    "@angular/platform-browser-dynamic": "^14.2.6",
    "@angular/router": "^14.2.6",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@ngrx/component": "^14.3.2",
    "@ngrx/component-store": "^14.3.2",
    "@ngrx/data": "^14.3.2",
    "@ngrx/effects": "^14.3.2",
    "@ngrx/entity": "^14.3.2",
    "@ngrx/router-store": "^14.3.2",
    "@ngrx/store": "^14.3.2",
    "@ngrx/store-devtools": "^14.3.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "jwt-decode": "^3.1.2",
    "rxjs": "^7.5.7",
    "tslib": "^2.4.0",
    "typesync": "^0.9.2",
    "webpack": "^5.74.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular-devkit/schematics": "^14.2.6",
    "@angular/cli": "^14.2.6",
    "@angular/compiler-cli": "^14.2.6",
    "@angular/localize": "^14.2.6",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngrx/schematics": "^14.3.2",
    "@types/jasmine": "^4.3.0",
    "@types/karma": "^6.3.3",
    "@types/karma-chrome-launcher": "^3.1.1",
    "@types/karma-coverage": "^2.0.1",
    "@types/karma-jasmine": "^4.0.2",
    "@types/karma-jasmine-html-reporter": "^1.7.0",
    "@types/node": "^18.11.0",
    "@types/yargs": "^17.0.13",
    "codelyzer": "^6.0.2",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "dotenv-expand": "9.0.0",
    "jasmine-core": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "protractor": "^7.0.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "~4.8.4",
    "yargs": "^17.6.0"
  }
}
