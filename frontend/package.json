{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "dotenv -e ../backend/.env ts-node ./scripts/setenv.ts",
    "start": "npm run config -- --environment=dev && ng serve --proxy-config proxy.conf.json --port 4200",
    "build": "npm run config -- --environment=prod && ng build",
    "preinstall": "typesync || :",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,da,de,fi,nb,nl,sv}.json --clean --format json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.2",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/forms": "^11.1.2",
    "@angular/material": "^11.1.1",
    "@angular/platform-browser": "^11.1.2",
    "@angular/platform-browser-dynamic": "^11.1.2",
    "@angular/router": "^11.1.2",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@ngrx/component": "^10.1.2",
    "@ngrx/component-store": "^10.1.2",
    "@ngrx/data": "^10.1.2",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/entity": "^10.1.2",
    "@ngrx/router-store": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "jwt-decode": "^3.1.2",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "typesync": "^0.8.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.2",
    "@angular/cli": "^11.1.2",
    "@angular/compiler-cli": "^11.1.2",
    "@angular/localize": "^11.1.2",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@ngrx/schematics": "^10.1.2",
    "@types/jasmine": "^3.6.3",
    "@types/karma": "^5.0.1",
    "@types/karma-chrome-launcher": "^3.1.0",
    "@types/karma-coverage": "^1.1.2",
    "@types/karma-jasmine": "^3.1.0",
    "@types/karma-jasmine-html-reporter": "^1.5.1",
    "@types/node": "^14.14.25",
    "@types/yargs": "^16.0.0",
    "codelyzer": "^6.0.1",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^7.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "~4.0.5",
    "yargs": "^16.2.0"
  }
}
