{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "dotenv -e ../.env ts-node ./scripts/setenv.ts",
    "start": "npm run config -- --environment=dev && ng serve --proxy-config proxy.conf.json --port 4200",
    "build": "npm run config -- --environment=prod && ng build",
    "preinstall": "typesync || :",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,da,de,fi,nb,nl,sv}.json --clean --format json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.4",
    "@angular/cdk": "^12.0.4",
    "@angular/common": "^12.0.4",
    "@angular/compiler": "^12.0.4",
    "@angular/core": "^12.0.4",
    "@angular/forms": "^12.0.4",
    "@angular/material": "^12.0.4",
    "@angular/platform-browser": "^12.0.4",
    "@angular/platform-browser-dynamic": "^12.0.4",
    "@angular/router": "^12.0.4",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ngrx/component": "^12.1.0",
    "@ngrx/component-store": "^12.1.0",
    "@ngrx/data": "^12.1.0",
    "@ngrx/effects": "^12.1.0",
    "@ngrx/entity": "^12.1.0",
    "@ngrx/router-store": "^12.1.0",
    "@ngrx/store": "^12.1.0",
    "@ngrx/store-devtools": "^12.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "jwt-decode": "^3.1.2",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "typesync": "^0.8.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.4",
    "@angular-devkit/schematics": "^12.0.4",
    "@angular/cli": "^12.0.4",
    "@angular/compiler-cli": "^12.0.4",
    "@angular/localize": "^12.0.4",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngrx/schematics": "^12.1.0",
    "@types/jasmine": "^3.7.7",
    "@types/karma": "^6.3.0",
    "@types/karma-chrome-launcher": "^3.1.0",
    "@types/karma-coverage": "^2.0.0",
    "@types/karma-jasmine": "^4.0.0",
    "@types/karma-jasmine-html-reporter": "^1.5.1",
    "@types/node": "^15.12.2",
    "@types/yargs": "^17.0.0",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "protractor": "^7.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "~4.2.4",
    "yargs": "^17.0.1"
  }
}
