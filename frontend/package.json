{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "dotenv -e ../.env ts-node ./scripts/setenv.ts",
    "start": "npm run config -- --environment=dev && ng serve --proxy-config proxy.conf.json --port 4800",
    "build": "npm run config -- --environment=prod && ng build",
    "pnpm-instal": "pnpm install --no-frozen-lockfile",
    "nx": "pnpm nx",
    "nxupdate": "nx migrate latest",
    "nxmigrate": "nx migrate --run-migrations",
    "pnpm-auto-update": "pnpm update",
    "preinstall": "typesync || :",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{en,da,de,fi,nb,nl,sv}.json --clean --format json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.1",
    "@fortawesome/free-brands-svg-icons": "^6.7.1",
    "@fortawesome/free-regular-svg-icons": "^6.7.1",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@ngrx/component": "^18.1.1",
    "@ngrx/component-store": "^18.1.1",
    "@ngrx/data": "^18.1.1",
    "@ngrx/effects": "^18.1.1",
    "@ngrx/entity": "^18.1.1",
    "@ngrx/router-store": "^18.1.1",
    "@ngrx/store": "^18.1.1",
    "@ngrx/store-devtools": "^18.1.1",
    "@ngx-translate/core": "^16.0.3",
    "@ngx-translate/http-loader": "^16.0.0",
    "jwt-decode": "^4.0.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "typesync": "^0.13.4",
    "webpack": "^5.96.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.1",
    "@angular-devkit/schematics": "^19.0.1",
    "@angular/cli": "^19.0.1",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/localize": "^19.0.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngrx/schematics": "^18.1.1",
    "@types/jasmine": "^5.1.4",
    "@types/karma": "^6.3.9",
    "@types/karma-chrome-launcher": "^3.1.4",
    "@types/karma-coverage": "^2.0.3",
    "@types/karma-jasmine": "^4.0.5",
    "@types/karma-jasmine-html-reporter": "^1.7.3",
    "@types/node": "^22.9.3",
    "@types/yargs": "^17.0.33",
    "codelyzer": "^6.0.2",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.4",
    "dotenv-expand": "12.0.1",
    "jasmine-core": "^5.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "pnpm": "^9.14.2",
    "protractor": "^7.0.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "~5.6.3",
    "yargs": "^17.7.2"
  }
}
